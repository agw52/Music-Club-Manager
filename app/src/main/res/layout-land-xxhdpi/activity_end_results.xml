<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".EndResultsActivity">
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/endbackground"
        />
    <View
        android:layout_width="536dp"
        android:layout_height="236dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="31dp"
        android:layout_marginLeft="121dp"
        android:id="@+id/endgamecontainer"
        android:background="@drawable/alternativetable"
        android:backgroundTint="#BE38D9"
        android:alpha="0.55"
        android:visibility="invisible"
        />
    <View
        app:layout_constraintTop_toTopOf="@id/endgamecontainer"
        app:layout_constraintLeft_toLeftOf="@id/endgamecontainer"
        android:layout_marginLeft="104dp"
        android:layout_marginTop="35dp"
        android:background="@drawable/statview"
        android:id="@+id/firstline"
        android:layout_height="3dp"
        android:layout_width="329dp"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="130dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/firstline"
        android:layout_marginBottom="3dp"
        android:fontFamily="cursive"
        android:text="Lesson Sales"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/lessonsalestext"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="130dp"
        android:layout_height="26dp"
        app:layout_constraintRight_toRightOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/firstline"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="4dp"
        android:fontFamily="cursive"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/lessonsalesnumber"
        />
    <View
        app:layout_constraintTop_toBottomOf="@id/firstline"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        android:layout_marginTop="52dp"
        android:background="@drawable/statview"
        android:id="@+id/secondline"
        android:layout_height="3dp"
        android:layout_width="329dp"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/secondline"
        android:layout_marginBottom="3dp"
        android:fontFamily="cursive"
        android:text="Gift Costs"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/giftcoststext"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="130dp"
        android:layout_height="26dp"
        app:layout_constraintRight_toRightOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/secondline"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="4dp"
        android:fontFamily="cursive"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/giftcostsnumber"
        />
    <View
        app:layout_constraintTop_toBottomOf="@id/secondline"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        android:layout_marginTop="52dp"
        android:background="@drawable/statview"
        android:id="@+id/thirdline"
        android:layout_height="3dp"
        android:layout_width="329dp"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="100dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/thirdline"
        android:layout_marginBottom="3dp"
        android:fontFamily="cursive"
        android:text="Staff Costs"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/staffcoststext"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="130dp"
        android:layout_height="26dp"
        app:layout_constraintRight_toRightOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/thirdline"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="4dp"
        android:fontFamily="cursive"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/staffcostsnumber"
        />
    <View
        app:layout_constraintTop_toBottomOf="@id/thirdline"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        android:layout_marginTop="72dp"
        android:background="@drawable/statview"
        android:id="@+id/fourthline"
        android:layout_height="3dp"
        android:layout_width="329dp"
        android:backgroundTint="#AFA409"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="60dp"
        android:layout_height="30dp"
        app:layout_constraintLeft_toLeftOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/fourthline"
        android:layout_marginBottom="3dp"
        android:fontFamily="cursive"
        android:text="Profit"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/profittext"
        android:visibility="invisible"
        />
    <TextView
        android:layout_width="130dp"
        android:layout_height="26dp"
        app:layout_constraintRight_toRightOf="@id/firstline"
        app:layout_constraintBottom_toTopOf="@id/fourthline"
        android:layout_marginBottom="3dp"
        android:layout_marginRight="4dp"
        android:fontFamily="cursive"
        android:textSize="25sp"
        android:textColor="@color/white"
        android:id="@+id/profitnumber"
        />
    <Button
        android:layout_width="140dp"
        android:layout_height="61dp"
        app:layout_constraintTop_toBottomOf="@id/endgamecontainer"
        app:layout_constraintLeft_toLeftOf="@id/endgamecontainer"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="198dp"
        android:backgroundTint="#389CD9"
        android:fontFamily="cursive"
        android:text="Main Menu"
        android:textSize="18sp"
        android:id="@+id/endgamebutton"
        android:visibility="invisible"
        />
</androidx.constraintlayout.widget.ConstraintLayout>